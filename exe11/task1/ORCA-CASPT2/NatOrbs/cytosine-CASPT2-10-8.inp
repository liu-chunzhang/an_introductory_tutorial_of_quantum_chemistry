! TZVP Usesym MOread NoFrozenCore
%moinp	"cytosine-RHF.gbw"

%pal
	nprocs	32
end

%scf
    rotate
            {20,25} {23,26} {30,33} {31,38}
    end
end

%casscf
	nel	        10
	norb        8 
	irrep       0
    mult        1
    NRoots      6
    gtol        1e-5
    etol        1e-9
    Maxiter     200
    PTMethod    FIC_CASPT2
    PTsettings  
        CASPT2_IPEAshift    0.25
        CASPT2_rshift       0.10
    end
end

